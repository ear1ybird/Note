<div name=3D"code" class=3D"code">bool intersect(const Ray &amp;r)
{
    float tmin =3D (min.x - r.orig.x) / r.dir.x;
    float tmax =3D (max.x - r.orig.x) / r.dir.x;

    if (tmin &gt; tmax) swap(tmin, tmax);

    float tymin =3D (min.y - r.orig.y) / r.dir.y;
    float tymax =3D (max.y - r.orig.y) / r.dir.y;

    if (tymin &gt; tymax) swap(tymin, tymax);

    if ((tmin &gt; tymax) || (tymin &gt; tmax))
        return false;

    if (tymin &gt; tmin)
        tmin =3D tymin;

    if (tymax &lt; tmax)
        tmax =3D tymax;

    float tzmin =3D (min.z - r.orig.z) / r.dir.z;
    float tzmax =3D (max.z - r.orig.z) / r.dir.z;

    if (tzmin &gt; tzmax) swap(tzmin, tzmax);

    if ((tmin &gt; tzmax) || (tzmin &gt; tmax))
        return false;

    if (tzmin &gt; tmin)
        tmin =3D tzmin;

    if (tzmax &lt; tmax)
        tmax =3D tzmax;

