    tmax =3D (bounds[1-r.sign[0]].x - r.orig.x) * r.invdir.x;
    tymin =3D (bounds[r.sign[1]].y - r.orig.y) * r.invdir.y;
    tymax =3D (bounds[1-r.sign[1]].y - r.orig.y) * r.invdir.y;
   =20
    if ((tmin &gt; tymax) || (tymin &gt; tmax))
        return false;

    if (tymin &gt; tmin)
        tmin =3D tymin;
    if (tymax &lt; tmax)
        tmax =3D tymax;
   =20
    tzmin =3D (bounds[r.sign[2]].z - r.orig.z) * r.invdir.z;
    tzmax =3D (bounds[1-r.sign[2]].z - r.orig.z) * r.invdir.z;
   =20
    if ((tmin &gt; tzmax) || (tzmin &gt; tmax))
        return false;

    if (tzmin &gt; tmin)
        tmin =3D tzmin;
    if (tzmax &lt; tmax)
        tmax =3D tzmax;

    return true;
}</t></t></div>

<p>The optimized version runs on our machine approximately 1.5 faster than =
the first version (the speedup depends on the compiler).</p>

