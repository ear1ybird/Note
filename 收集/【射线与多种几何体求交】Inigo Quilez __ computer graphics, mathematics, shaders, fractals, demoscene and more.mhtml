gfx16.png"><br>
<b>Rounded Box</b><br>
<a href=3D"https://www.shadertoy.com/view/WlSXRW">Shadertoy example</a>
</div>
<div class=3D"part">
<div class=3D"code"><span class=3D"codeComment">// axis aligned box centere=
d at the origin, with dimensions "size" and extruded by radious "rad"
</span>float roundedboxIntersect( in vec3 ro, in vec3 rd, in vec3 size, in =
float rad )
{
    <span class=3D"codeComment">// bounding box</span>
    vec3 m =3D 1.0/rd;
    vec3 n =3D m*ro;
    vec3 k =3D abs(m)*(size+rad);
    vec3 t1 =3D -n - k;
    vec3 t2 =3D -n + k;
    float tN =3D max( max( t1.x, t1.y ), t1.z );
    float tF =3D min( min( t2.x, t2.y ), t2.z );
    if( tN&gt;tF || tF&lt;0.0) return -1.0;
    float t =3D tN;

    <span class=3D"codeComment">// convert to first octant</span>
    vec3 pos =3D ro+t*rd;
    vec3 s =3D sign(pos);
    ro  *=3D s;
    rd  *=3D s;
    pos *=3D s;
       =20
    <span class=3D"codeComment">// faces</span>
    pos -=3D size;
    pos =3D max( pos.xyz, pos.yzx );
    if( min(min(pos.x,pos.y),pos.z) &lt; 0.0 ) return t;

    <span class=3D"codeComment">// some precomputation</span>
    vec3 oc =3D ro - size;
    vec3 dd =3D rd*rd;
    vec3 oo =3D oc*oc;
    vec3 od =3D oc*rd;
    float ra2 =3D rad*rad;

    t =3D 1e20;       =20

    <span class=3D"codeComment">// corner</span>
    {
    float b =3D od.x + od.y + od.z;
    float c =3D oo.x + oo.y + oo.z - ra2;
    float h =3D b*b - c;
    if( h&gt;0.0 ) t =3D -b-sqrt(h);
    }
    <span class=3D"codeComment">// edge X</span>
    {
    float a =3D dd.y + dd.z;
    float b =3D od.y + od.z;
    float c =3D oo.y + oo.z - ra2;
    float h =3D b*b - a*c;
    if( h&gt;0.0 )
    {
        h =3D (-b-sqrt(h))/a;
        if( h&gt;0.0 &amp;&amp; h&lt;t &amp;&amp; abs(ro.x+rd.x*h)&lt;size.=
x ) t =3D h;
    }
    }
    <span class=3D"codeComment">// edge Y</span>
    {
    float a =3D dd.z + dd.x;
    float b =3D od.z + od.x;
    float c =3D oo.z + oo.x - ra2;
    float h =3D b*b - a*c;
    if( h&gt;0.0 )
    {
        h =3D (-b-sqrt(h))/a;
        if( h&gt;0.0 &amp;&amp; h&lt;t &amp;&amp; abs(ro.y+rd.y*h)&lt;size.=
y ) t =3D h;
    }
    }
    <span class=3D"codeComment">// edge Z</span>
    {
    float a =3D dd.x + dd.y;
    float b =3D od.x + od.y;
    float c =3D oo.x + oo.y - ra2;
    float h =3D b*b - a*c;
    if( h&gt;0.0 )
    {
        h =3D (-b-sqrt(h))/a;
        if( h&gt;0.0 &amp;&amp; h&lt;t &amp;&amp; abs(ro.z+rd.z*h)&lt;size.=
z ) t =3D h;
    }
    }

    if( t&gt;1e19 ) t=3D-1.0;
   =20
    return t;
}

<span class=3D"codeComment">// normal of a rounded box</span>
vec3 roundedboxNormal( in vec3 pos, in vec3 siz, in float rad )
{
    return sign(pos)*normalize(max(abs(pos)-siz,0.0));
}</div>
