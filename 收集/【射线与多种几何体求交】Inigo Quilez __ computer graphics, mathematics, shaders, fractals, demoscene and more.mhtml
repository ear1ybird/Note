<div class=3D"code"><span class=3D"codeComment">// axis aligned box centere=
d at the origin, with size boxSize
</span>vec2 boxIntersection( in vec3 ro, in vec3 rd, vec3 boxSize, out vec3=
 outNormal )=20
{
    vec3 m =3D 1.0/rd; <span class=3D"codeComment">// can precompute if tra=
versing a set of aligned boxes</span>
    vec3 n =3D m*ro;   <span class=3D"codeComment">// can precompute if tra=
versing a set of aligned boxes</span>
    vec3 k =3D abs(m)*boxSize;
    vec3 t1 =3D -n - k;
    vec3 t2 =3D -n + k;
    float tN =3D max( max( t1.x, t1.y ), t1.z );
    float tF =3D min( min( t2.x, t2.y ), t2.z );
    if( tN&gt;tF || tF&lt;0.0) return vec2(-1.0); <span class=3D"codeCommen=
t">// no intersection</span>
    outNormal =3D -sign(rd)*step(t1.yzx,t1.xyz)*step(t1.zxy,t1.xyz);
    return vec2( tN, tF );
}</div>
</div>
